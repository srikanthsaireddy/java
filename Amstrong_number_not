import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
  Scanner s=new Scanner(System.in);
  int n=s.nextInt();
  int number=n;
  int sum=0;
  while(n!=0)
  {
      int r=n%10;
      sum=sum+(r*r*r);
      n=n/10;
  }
  if(number==sum)
  {
      System.out.println("Amstrong");
  }
  else{
      System.out.println("not Amstrong");
  }
	}
}
