import java.util.Scanner;
public class Main
{
  public static void main (String[]args) 
  {
    Scanner sc = new Scanner (System.in);
    int n=sc.nextInt();
    if(n>=0)
    {
       int fact=1;
       for(int i=n;i>=1;i--)
       {
           fact=fact*i;
       }
       System.out.println(fact);
    }
  }
}
