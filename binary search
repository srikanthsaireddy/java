geeksforgeeks code:
class Solution {
    int binarysearch(int arr[], int n, int k){
        // code here
        int result=0;
        for(int i=0;i<n;i++)
        {
            if(k==arr[i])
            {
                result=i;
                break;
            }
            else if(k>n)
            {
                result=-1;
            }
        }
        return result;
    }
}
